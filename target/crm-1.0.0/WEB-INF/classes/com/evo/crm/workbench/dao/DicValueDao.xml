<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.evo.crm.workbench.dao.DicValueDao">

    <select id="selectTypeCode" resultType="com.evo.crm.workbench.domain.DicValue">
        select * from tbl_dic_value where typeCode = #{code} order by orderNo
    </select>

    <select id="selectAll" resultType="java.lang.String">
        select * from tbl_dic_value
    </select>
</mapper>
